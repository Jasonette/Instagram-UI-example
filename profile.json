                    "font": "HelveticaNeue-Bold",
                    "size": "12"
                  }
                }]
              }, {
                "type": "vertical",
                "style": {
                  "padding": "0",
                  "spacing": "10"
                },
                "components": [{
                  "type": "horizontal",
                  "style": {
                    "distribution": "equalsize",
                    "padding": "0"
                  },
                  "components": {
                    "{{#each stats}}": {
                      "type": "vertical",
                      "style": {
                        "padding": "0",
                        "spacing": "0",
                        "align": "center"
                      },
                      "components": [{
                        "type": "label",
                        "text": "{{number}}",
                        "style": {
                          "font": "HelveticaNeue-Bold",
                          "size": "16"
                        }
                      }, {
                        "type": "label",
                        "text": "{{name}}",
                        "style": {
                          "color": "#bdbdbd",
                          "size": "10"
                        }
                      }]
                    }
                  }
                }, {
                  "type": "button",
                  "text": "Edit Profile",
                  "style": {
                    "corner_radius": "3",
                    "height": "30",
                    "size": "12",
                    "color": "#000000",
