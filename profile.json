                      "text": "{{name}}",
                      "style": {
                        "font": "HelveticaNeue-Bold",
                        "size": "12"
                      }
                    }, {
                      "type": "label",
                      "text": "{{location}} >",
                      "style": {
                        "font": "HelveticaNeue",
                        "size": "11"
                      }
                    }]
                  }, {
                    "type": "space"
                  }, {
                    "type": "image",
                    "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/dotdotdot.png",
                    "style": {
                      "padding_top": "10",
                      "width": "25"
                    }
                  }]
                }, {
                  "type": "image",
                  "url": "{{content}}",
                  "style": {
                    "width": "100%",
                    "padding": "0"
                  }
                }]
